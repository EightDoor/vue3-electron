{
  "name": "vite-project",
  "version": "0.0.0",
  "main": "main.js",
  "homepage": "./electron",
  "scripts": {
    "z": "concurrently  \"npm run watch\" \"npm run dev\" \"npm run start\"",
    "start:electron": "electron ./electron",
    "start": "wait-on -c waitOnConfig.json  http-get://localhost:9999/ -v && electron ./electron",
    "watch": "tsc --project ./electron/tsconfig.json -w",
    "tsc": "tsc --project ./electron/tsconfig.json",
    "dev": "vite",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "serve": "vite preview",
    "lint": "yarn prettier --write",
    "pack": "electron-builder --dir",
    "dist:all": "electron-builder -mwl",
    "dist:mac": "electron-builder --mac",
    "dist:mac_all": "concurrently  \"npm run build\" \"npm run tsc\" \"npm run dist:mac\"",
    "dist:win": "electron-builder --win --x64",
    "postinstall": "electron-builder install-app-deps",
    "dist:linux": "electron-builder --linux deb tar.xz AppImage"
  },
  "author": "周凯 <851708184@qq.com>",
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "element-plus": "^1.2.0-beta.2",
    "vue": "^3.2.21",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/electron-is-dev": "^1.1.1",
    "@types/node": "^16.11.6",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/compiler-sfc": "^3.2.21",
    "concurrently": "^6.3.0",
    "debugout.js": "^1.0.0",
    "electron": "^15.3.0",
    "electron-builder": "^22.13.1",
    "electron-log": "^4.4.1",
    "electron-rebuild": "^3.2.3",
    "electron-reload": "^2.0.0-alpha.1",
    "less": "^4.1.2",
    "prettier": "2.4.1",
    "typescript": "^4.4.4",
    "unplugin-vue-components": "^0.17.2",
    "vite": "^2.6.13",
    "vite-plugin-style-import": "^1.3.0",
    "vue-tsc": "^0.28.10",
    "wait-on": "^6.0.0"
  },
  "linux": {
    "icon": "dist/icons"
  },
  "packageManager": "yarn@3.1.0"
}
